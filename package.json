{
  "name": "bitbucket-agent-cli",
  "version": "0.2.1",
  "description": "Bitbucket CLI for AI agents (and maybe humans).",
  "keywords": [
    "bitbucket",
    "bun",
    "cli",
    "code-review",
    "pull-request"
  ],
  "license": "MIT",
  "author": "Luis Rudge",
  "repository": {
    "type": "git",
    "url": "https://github.com/luisrudge/bitbucket-agent-cli"
  },
  "bin": {
    "bitbucket-agent-cli": "./dist/cli.js"
  },
  "files": [
    "dist/cli.js"
  ],
  "type": "module",
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun build ./src/index.ts --outfile dist/cli.js --target bun && node -e \"const fs=require('fs'); const f='dist/cli.js'; fs.writeFileSync(f, '#!/usr/bin/env bun\\n' + fs.readFileSync(f))\"",
    "prepublishOnly": "bun run build",
    "type-check": "tsgo --noEmit",
    "lint": "oxlint --fix",
    "lint:check": "oxlint",
    "format": "oxfmt",
    "format:check": "oxfmt --check",
    "test": "bun test"
  },
  "dependencies": {
    "commander": "^14.0.2"
  },
  "devDependencies": {
    "@types/bun": "^1.3.5",
    "@typescript/native-preview": "^7.0.0-dev.20260112.1",
    "lefthook": "^2.0.14",
    "oxfmt": "^0.24.0",
    "oxlint": "1.39.0"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}
